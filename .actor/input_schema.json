{
  "title": "Datagol Bridge Input",
  "description": "Actor to bridge LinkedIn job scrapes with Datagol.",
  "type": "object",
  "schemaVersion": 1,
  "properties": {
    "timeout": {
      "title": "Actor Timeout (seconds)",
      "type": "integer",
      "description": "Maximum run time for the entire actor in seconds.",
      "default": 3600,
      "minimum": 60,
      "maximum": 86400
    },
    "DATAGOL_READ_TOKEN": {
        "title": "Datagol API Read Token",
        "type": "string",
        "description": "Read Token",
        "default": "eyJhbGciOiJIUzUxMiJ9..pv9zfQa2Ojk8aHVIMOynXzCdwGWExi82dPj5-cqsJ5sbSEeGvdxW01QFiL1lEYTx4xn_G14BxWALnH32aX5eaw",
        "editor": "textfield"
    },
    "DATAGOL_WRITE_TOKEN": {
        "title": "Datagol API Write Token",
        "type": "string",
        "description": "Write Token",
        "default": "8c780b764720e57acbe59edbfce007f5e5814a4a42c86b319af939b787380326",
        "editor": "textfield"
    },
    "datagolApiBaseUrl": {
      "title": "Datagol API Base URL",
      "type": "string",
      "description": "Base URL for the Datagol API.",
      "default": "https://be-eu.datagol.ai/noCo/api/v2",
      "editor": "textfield"
    },
    "datagolApiWorkspaceId": {
      "title": "Datagol Workspace ID",
      "type": "string",
      "description": "Workspace ID in Datagol (or set DATAGOL_WORKSPACE_ID env var).",
      "default": "8e894b95-cc40-44f0-88d7-4aae346b0325",
      "editor": "textfield"
    },
    "jobTitlesTableId": {
      "title": "Job Titles Table ID",
      "type": "string",
      "description": "Datagol table ID for job titles.",
      "default": "395a586f-2d3e-4489-a5d9-be0039f97aa1",
      "editor": "textfield"
    },
    "excludedCompaniesTableId": {
      "title": "Excluded Companies Table ID",
      "type": "string",
      "description": "Datagol table ID for the excluded companies list.",
      "default": "ac27bdbc-b564-429e-815d-356d58b00d06",
      "editor": "textfield"
    },
    "locationsTableId": {
      "title": "Locations Table ID",
      "type": "string",
      "description": "Datagol table ID for locations.",
      "default": "6122189a-764f-40a9-9721-d756b7dd3626",
      "editor": "textfield"
    },
    "jobPostingsTableId": {
      "title": "Job Postings Table ID",
