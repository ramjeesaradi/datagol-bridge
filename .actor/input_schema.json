      "default": 50
    },
    "scraperTimeoutSecs": {
      "title": "Scraper Timeout (seconds)",
      "type": "integer",
      "description": "Timeout for each individual LinkedIn scraper run.",
      "default": 600
    },
    "maxConcurrentScrapers": {
      "title": "Max Concurrent Scrapers",
      "type": "integer",
      "description": "Maximum number of scrapers to run in parallel.",
      "default": 30
    },
    "jobTitles": {
